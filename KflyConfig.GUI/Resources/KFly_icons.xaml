<ResourceDictionary xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
    <Canvas Width="20" Height="20" x:Key="line_left_down">
        <Line X1="2" Y1="10" X2="10" Y2="10" Stroke="Black" StrokeThickness="2" />
        <Line X1="10" Y1="10" X2="10" Y2="18" Stroke="Black" StrokeThickness="1" />
    </Canvas>
    <Canvas Width="20" Height="20" x:Key="line_left_up">
        <Line X1="2" Y1="10" X2="10" Y2="10" Stroke="Black" StrokeThickness="2" />
        <Line X1="10" Y1="10" X2="10" Y2="2" Stroke="Black" StrokeThickness="1" />
    </Canvas>
</ResourceDictionary>